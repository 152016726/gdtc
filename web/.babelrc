{
  "presets": ["es2015"],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [{
          "rootPathPrefix": "~",
          "rootPathSuffix": "./src"
        },{
          "rootPathPrefix": "#",
          "rootPathSuffix": "./common_modules"
        }]
      }
    ]
  ]
}